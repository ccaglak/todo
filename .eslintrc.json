{
   "parser": "@babel/eslint-parser",
   "plugins": [
      "jest"
   ],
   "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
   ],
   "env": {
      "browser": true,
      "es6": true,
      "node": true
   },
   "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
   },
   "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true
      },
      "rules": {
         "no-console": "off"
      }
   }
}
